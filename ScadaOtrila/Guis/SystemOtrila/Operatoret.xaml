        <PasswordBox x:Name="txtPassword" HorizontalAlignment="Left" Height="31" Margin="263,107,0,0" Password="admin123" VerticalAlignment="Top" Width="168"/>
        <Label Content="Fjalëkalimi" HorizontalAlignment="Left" Height="31" Margin="148,107,0,0" VerticalAlignment="Top" Width="110"/>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Left" Height="31" Margin="263,143,0,0" TextWrapping="Wrap" Text="@gmail.com" VerticalAlignment="Top" Width="168"/>
        <Label Content="E-mail" HorizontalAlignment="Left" Height="31" Margin="148,143,0,0" VerticalAlignment="Top" Width="110"/>

    </Grid>
</Window>
