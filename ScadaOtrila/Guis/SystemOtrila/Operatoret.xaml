        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Height="31" Margin="263,35,0,0" TextWrapping="Wrap" Text="admin123" VerticalAlignment="Top" Width="168"/>
        <Label Content="Përdoruesi" HorizontalAlignment="Left" Height="31" Margin="148,35,0,0" VerticalAlignment="Top" Width="110"/>
