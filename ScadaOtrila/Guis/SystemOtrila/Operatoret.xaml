        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Operatoret" Height="280.347" Width="458.171">
    <Grid>
        <ListBox x:Name="listLogins" HorizontalAlignment="Left" Margin="10,35,0,0" Width="132" Height="198" VerticalAlignment="Top" SelectionChanged="ListLogins_SelectionChanged"/>
        <Label Content="Lista e operatorve" HorizontalAlignment="Left" Height="25" Margin="10,5,0,0" VerticalAlignment="Top" Width="132"/>
        <Button x:Name="btnRuaj" Content="RUAJ" HorizontalAlignment="Left" Margin="298,196,0,18" Width="133" Click="BtnRuaj_Click"/>
